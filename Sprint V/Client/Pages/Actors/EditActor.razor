@page "/actors/edit/{ActorId:int}"
@using PeliculasCODE5G.Client.Pages.Components

<FormActor OnValidSubmit="Edit" Actor="Actor"></FormActor>
@code{
    [Parameter] public int ActorId {get;set;}
    Actor Actor = new Actor();
    protected override void OnInitialized(){
        Actor = new Actor(){
            Id = ActorId,
            ActorName = "Will Smith",
            BirthDate = DateTime.Today
        };
    }
    private void Edit(){
        Console.WriteLine("Editando actor");
    }
}